n  >\n    <v-btn\n      icon\n      x-small\n      color=\"black\"\n      class=\"globaloverviewTrigger\"\n      @click=\"toggleMenuShow\"\n    >\n      <v-icon>mdi-format-align-left</v-icon>\n    </v-btn>\n    <v-slide-y-transition>\n      <template v-if=\"showMenuBar\">\n        <div>\n          <v-card\n            outlined\n          >\n            <v-row\n              v-for=\"tool in tools\"\n              :key=\"tool.label\"\n              no-gutters\n            >\n              <v-col>\n                <GlobalviewMenuBtn\n                  :item=\"tool\"\n                  @toolEnableToggle=\"toolEnableToggleHandler\"\n                />\n\n              </v-col>\n            </v-row>\n          </v-card>\n        </div>\n      </template>\n    </v-slide-y-transition>\n  </div>\n</template>\n\n<script>\nimport {mapState} from 'vuex'\nimport GlobalviewMenuBtn from './GlobalviewMenuBtn.vue'\nexport default {\n  data(){\n    return {\n      items: [],\n      showMenuBar: false, \n      tools: [\n        {\n          label: 'lasso', \n          icon: 'mdi-lasso',\n          active: false, \n        }, \n      ]\n    }\n  },\n  methods: {\n    toggleMenuShow() {\n      this.showMenuBar = !this.showMenuBar\n    },\n    toolEnableToggleHandler(e){\n      this.$emit('toolEnableToggle', e)\n    }\n  },\n  computed: {\n  },\n  components: {\n    GlobalviewMenuBtn,\n  },\n  created(){\n  }\n}\n</script>\n\n<style scoped>\n  .globaloverview-menu{\n    position: absolute; \n    top: 5px;\n    left: 10px;\n  }\n  .globaloverviewTrigger{\n    margin-left: 7px;\n  }\n</style>"]}]}