geCurrentDraggingVM({commit}, vm){\n      commit('SET_CRRENTDRAGGING', vm)\n    },\n    setCurrentLink({commit}, {id, pos, in_out}){\n      commit('SET_CURRENTLINK', {id, pos, in_out})\n    }, \n    toggleVisMode({commit}) {\n      commit('TOGGLE_VIS_MODE')\n    }\n  },\n  modules: {\n    menu,\n    topics,\n    documents,\n    globalview,\n    corpus,\n    link, \n    ontology,\n    graph,\n    vegaRender, \n    table2cypher,\n    graphviewer\n  }\n})\n"]}]}